        "wdio": "wdio wdio.test.conf.ts ",
        "ch2": "yarn wdio --spec ./test/specs/ch2.ts",
        "ch3": "yarn wdio --spec ./test/specs/ch3.ts",
        "ch4": "yarn wdio --spec ./test/specs/ch4.ts",
        "ch5": "yarn wdio --spec ./test/specs/ch5.ts",
        "ch8": "yarn wdio --spec ./test/specs/ch8.ts",
        "ch12": "yarn wdio --spec ./test/specs/ch12.ts",
        "ch13": "yarn wdio --spec ./test/specs/ch13.ts",
        "ch14": "cross-env Env=dev yarn wdio --spec ./test/specs/ch14.ts",
        "ch15": "cross-env Env=dev wdio wdio.lambdatest.conf.ts --spec ./test/specs/ch14.ts",
        "uat": "yarn wdio --spec ./test/specs/ch3.ts",
        "dev": "Env=dev wdio wdio.dev.conf.ts --spec ./test/specs/ch13.ts",
        "prod": "Env=prod wdio wdio.prod.conf.ts --spec ./test/specs/ch2.ts"
    },
