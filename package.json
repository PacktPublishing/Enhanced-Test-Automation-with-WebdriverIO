        "expect-webdriverio": "^4.2.7",
        "geckodriver": "^4.0.5",
        "jasmine": "^5.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "utility-types": "^3.10.0",
        "wdio-chromedriver-service": "^8.1.1",
